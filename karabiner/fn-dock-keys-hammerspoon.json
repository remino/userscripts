{
	"title": "Fn Dock keys",
	"rules": [
		{
			"description": "Fn + num to focus Dock app (uses Hammerspoon)",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "1",
						"modifiers": {
							"mandatory": [
								"fn"
							]
						}
					},
					"to": [
						{
							"shell_command": "open 'hammerspoon://focusDockApp?n=1'"
						}
					]
				},
